<!DOCTYPE document PUBLIC "-//APACHE//DTD Documentation V1.3//EN" "http://forrest.apache.org/dtd/document-v13.dtd">
<document> 
  <header> 
    <title>Title</title> 
  </header> 
  <body> 
    <section>
      <title>Section Title</title>
      <p><link href="#samepage_target">same page target</link></p>
      <p><link href="#sometarget">target w/ name shared on other pages</link></p>
    </section>
    <section id="samepage_target">
       <title>part two</title>
      <p>1 2 4 34 4 blah blah content</p>
      <p>1 2 4 34 4 blah blah content</p>
     </section>
    <section id="sometarget">
      <title>a third part</title>
      <p>1 2 4 34 4 blah <link href="site:foo">deep link</link> content</p>
      <p>1 2 4 34 4 blah <link href="ext:forrest">external link</link> content</p>
      <p>1 2 4 34 4 blah <link href="http://www.apache.org/">normal external link</link> content</p>
      <p>1 2 4 34 4 blah blah content</p>
      <section id="subsection">
        <title>subsection</title>
        <p>this section is part of another</p>
      </section>
     </section>
  </body>
</document>
