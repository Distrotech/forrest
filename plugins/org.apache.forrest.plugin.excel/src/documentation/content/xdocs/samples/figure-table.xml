    <Cell><Data ss:Type="String">/dev/null</Data></Cell>
   </Row>
   <Row>
    <Cell ss:Index="2" ss:StyleID="s26"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s21"><Data ss:Type="String">figure</Data></Cell>
    <Cell ss:StyleID="s26"><Data ss:Type="String">shop.jpg</Data></Cell>
